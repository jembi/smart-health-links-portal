{
    "transporters": [
        {
            "transporterType": "file",
            "options": {
                "filename": "logs/logs-%DATE%.log",
                "datePattern": "YYYY-MM-DD",
                "zippedArchive": true,
                "maxSize": "20m",
                "maxFiles": "14d"
            }
        }
    ],
    "format": ["timestamp", "json"]
}